Include("emdata.h")

Factory = Template("EMAN::Factory", "emobject.h")
Function("EMAN::dump_reconstructors", "reconstructor.h")

Reconstructor = Class("EMAN::Reconstructor", "reconstructor.h")
set_policy(Reconstructor.finish, return_internal_reference())
rename(Reconstructor, "__Reconstructor")

ReconstructorFactory = Factory("EMAN::Reconstructor", "Reconstructors")
